{
  "extends": "./tsconfig.json",
  "exclude": [],
  "compilerOptions": {
    "outDir": "./dist",
    "strict": true,
    "pretty": false,
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": false,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "target": "es5"
  }
}
